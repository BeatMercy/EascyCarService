<?xml version="1.0" encoding="UTF-8"?>

<ehcache xsi:noNamespaceSchemaLocation="ehcache.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<!-- timeToLiveSeconds是缓存存活时间，秒，认证服务器是7天，这里减少为1天 -->
<cache timeToLiveSeconds="86400" maxEntriesLocalHeap="0" name="userTokenInfo"></cache>

<cache timeToLiveSeconds="86400" maxEntriesLocalHeap="0" name="carServiceOption"></cache>


</ehcache>